{
  "name": "@near-js/jsonrpc-client",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "check-types": "tsc --noEmit",
    "example": "tsx src/example.ts"
  },
  "dependencies": {
    "@near-js/jsonrpc-types": "*"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
