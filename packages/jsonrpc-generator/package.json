{
  "name": "@near-js/jsonrpc-generator",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "check-types": "tsc --noEmit",
    "generate": "tsx src/index.ts",
    "test": "jest"
  },
  "dependencies": {
    "openapi-typescript": "7.8.0",
    "ts-morph": "^26.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.14",
    "expect-type": "^1.2.2",
    "jest": "^30.0.5",
    "patch-package": "^8.0.0",
    "ts-jest": "^29.4.0",
    "tsup": "^8.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
