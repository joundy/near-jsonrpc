{
  "name": "@near-js/jsonrpc-types",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./methods": {
      "types": "./dist/methods.d.ts",
      "require": "./dist/methods.js",
      "import": "./dist/methods.mjs"
    },
    "./schemas": {
      "types": "./dist/schemas.d.ts",
      "require": "./dist/schemas.js",
      "import": "./dist/schemas.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./mapped-properties": {
      "types": "./dist/mapped-properties.d.ts",
      "require": "./dist/mapped-properties.js",
      "import": "./dist/mapped-properties.mjs"
    },
    "./zod-schemas": {
      "types": "./dist/zod-schemas.d.ts",
      "require": "./dist/zod-schemas.js",
      "import": "./dist/zod-schemas.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "check-types": "tsc --noEmit"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "expect-type": "^1.2.2",
    "zod": "^4.0.5"
  }
}
